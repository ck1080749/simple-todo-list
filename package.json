{
  "name": "simple-todo-list",
  "version": "0.1.2-alpha",
  "description": "A simple Electron-based todo-list.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.electron.simple-todo-list",
    "mac": {
      "category": "your.app.category.type",
      "icon": "icon.png"
    },
    "win": {
      "icon": "icon.png"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "grunt": "^1.4.1",
    "grunt-electron-installer": "^2.1.0"
  },
  "dependencies": {
    "lowdb": "^3.0.0",
    "moment": "^2.29.1"
  }
}
